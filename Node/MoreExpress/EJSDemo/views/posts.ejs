<% include partials/header%>
<h1>
	 Welcome to the post page!
</h1>

<% for( var i = 0 ; i < posts.length; i++){ %>
	<li>
		<%= posts[i].topic %> - <%= posts[i].author %>
	</li>
<% } %>

<% posts.forEach(function(post){ %>
	<li><b><%= post.topic %> - <%= post.author %></b></li>
<% }); %>

<% include partials/footer%>